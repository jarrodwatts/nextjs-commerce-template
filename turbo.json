{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "next-commerce#build": {
      "dependsOn": [
        "^build",
        "$COMMERCE_PROVIDER",
        "$NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN"
      ],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "prettier-fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "types": {
      "outputs": ["dist/**/*.d.ts"]
    }
  }
}
